{
  "compilerOptions": {
    "declaration": false,
    "esModuleInterop": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "allowSyntheticDefaultImports": true,
    "noEmit": true,
    "jsx": "preserve",
    "types": [],
    "baseUrl": "./",
    "strict": true,
    "moduleResolution": "Node16",
    "module": "Node16",
    "paths": {
      "@/*": ["./src/*"]
    },
    "maxNodeModuleJsDepth": 10,
    "typeRoots": ["./node_modules/@types"],
    "noImplicitAny": false
  },
  "include": ["src"],
  "exclude": ["node_modules"]
}
